#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    // Outer loop controls rows
    for(int i = 1; i <= N; i++) {

        // Inner loop controls columns (decreasing)
        for(int j = 1; j <= N - i + 1; j++) {
            
            // Convert number to alphabet
            printf("%c ", 'A' + j - 1);
        }

        // Move to next line
        printf("\n");
    }

    return 0;
}
