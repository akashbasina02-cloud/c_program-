#include <stdio.h>

int main() {
    int n, i, j;
    char ch;

    scanf("%d", &n);

    ch = 'A' + n - 1;   // starting character

    for (i = n; i >= 1; i--) {

        for (j = 1; j <= i; j++) {
            printf("%c ", ch);
        }

        printf("\n");
        ch--;   // move to previous alphabet
    }

    return 0;
}
