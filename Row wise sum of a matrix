#include <stdio.h>

int main() {
    int n, m;
    scanf("%d %d", &n, &m);   // Read rows and columns

    int arr[n][m];

    // Read matrix elements
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < m; j++) {
            scanf("%d", &arr[i][j]);
        }
    }

    // Calculate row-wise sum
    for(int i = 0; i < n; i++) {
        int sum = 0;   // Reset sum for each row

        for(int j = 0; j < m; j++) {
            sum += arr[i][j];   // Add row elements
        }

        printf("%d ", sum);   // Print row sum
    }

    return 0;
}
