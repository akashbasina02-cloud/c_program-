#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    // Loop through rows
    for(int i = 1; i <= N; i++) {

        // Loop through columns
        for(int j = 1; j <= N; j++) {

            // Odd row → print 0, Even row → print 1
            if(i % 2 == 1)
                printf("0 ");
            else
                printf("1 ");
        }

        // New line after each row
        printf("\n");
    }

    return 0;
}
