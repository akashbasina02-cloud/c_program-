#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    // Outer loop for rows
    for(int i = 1; i <= N; i++) {
        
        // Calculate the character to print
        char ch = 'A' + (N - i);

        // Inner loop for columns (decreasing)
        for(int j = 1; j <= N - i + 1; j++) {
            printf("%c ", ch);
        }

        // New line after each row
        printf("\n");
    }

    return 0;
}
