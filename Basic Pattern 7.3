#include <stdio.h>

int main() {
    int n, i, j, s;
    scanf("%d", &n);

    for (i = 0; i < n; i++) {

        // print leading spaces
        for (s = 0; s < i; s++) {
            printf(" ");
        }

        // print numbers
        for (j = n - i; j >= 1; j--) {
            printf("%d ", j);
        }

        printf("\n");
    }

    return 0;
}
