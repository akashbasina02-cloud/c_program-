#include <stdio.h>

int main() {
    int n, i, j;
    
    // Read input
    scanf("%d", &n);

    // Loop through rows
    for (i = 0; i < n; i++) {
        
        // Print leading spaces
        for (j = 0; j < i; j++) {
            printf(" ");
        }

        // Print numbers
        for (j = 0; j < n - i; j++) {
            printf("%d ", n - i);
        }

        // New line after each row
        printf("\n");
    }

    return 0;
}
