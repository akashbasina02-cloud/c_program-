#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    for (int i = 0; i < N; i++) {
        // Print numbers
        for (int j = 1; j <= N - i; j++) {
            printf("%d", j);
        }

        // Print stars
        for (int k = 0; k < i; k++) {
            printf("*");
        }

        printf("\n");
    }

    return 0;
}
