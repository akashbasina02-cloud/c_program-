#include <stdio.h>
#include <math.h>

int main() {
    int n, temp, digit, sum = 0, count = 0;
    
    scanf("%d", &n);
    temp = n;

    // Count digits
    while (temp != 0) {
        count++;
        temp /= 10;
    }

    temp = n;

    // Calculate Disarium sum
    while (temp != 0) {
        digit = temp % 10;
        sum += pow(digit, count);
        count--;
        temp /= 10;
    }

    if (sum == n)
        printf("True");
    else
        printf("False");

    return 0;
}
