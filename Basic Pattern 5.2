#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);   // Read input N

    // Loop for rows
    for (int i = 1; i <= n; i++) {

        // Print numbers from 1 to i
        for (int j = 1; j <= i; j++) {
            printf("%d", j);
        }

        // Print stars (N - i) times
        for (int k = 1; k <= n - i; k++) {
            printf("*");
        }

        // Move to next line
        printf("\n");
    }

    return 0;
}
