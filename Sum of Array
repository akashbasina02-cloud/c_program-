#include <stdio.h>

int main() {
    int n, i;
    int arr[100];
    int sum = 0;

    // Read size of array
    scanf("%d", &n);

    // Read array elements
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Calculate sum
    for(i = 0; i < n; i++) {
        sum = sum + arr[i];
    }

    // Print sum
    printf("%d", sum);

    return 0;
}
