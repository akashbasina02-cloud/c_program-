#include<stdio.h>

int main() {
    int n, i, z;
    int count = 0;

    scanf("%d", &n);   // Read size of array

    int arr[n];

    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);   // Read array elements
    }

    scanf("%d", &z);   // Read element to count

    for(i = 0; i < n; i++) {
        if(arr[i] == z) {
            count++;   // Increase count if match found
        }
    }

    printf("%d", count);   // Print result

    return 0;
}
