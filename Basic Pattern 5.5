#include <stdio.h>

int main() {
    int n, i, j;

    scanf("%d", &n);

    for (i = 1; i <= n; i++) {

        // print stars
        for (j = 1; j <= n - i; j++) {
            printf("*");
        }

        // print numbers
        for (j = 1; j <= i; j++) {
            printf("%d", i);
        }

        printf("\n");
    }

    return 0;
}
