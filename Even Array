#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);   // Read size of array

    int arr[n];
    int isEvenArray = 1;   // Assume array is even (1 = True)

    // Read array elements
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);

        if(arr[i] % 2 != 0) {   // If element is odd
            isEvenArray = 0;    // Mark as False
        }
    }

    if(isEvenArray == 1) {
        printf("True");
    } else {
        printf("False");
    }

    return 0;
}
