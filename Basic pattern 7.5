#include <stdio.h>

int main() {
    int n, i, j, s;
    char ch;

    scanf("%d", &n);

    for (i = 0; i < n; i++) {

        ch = 'A' + i;

        // leading spaces
        for (s = 0; s < i; s++) {
            printf(" ");
        }

        // print alphabets
        for (j = n - i; j >= 1; j--) {
            printf("%c ", ch);
        }

        printf("\n");
    }

    return 0;
}
