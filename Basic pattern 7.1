#include <stdio.h>

int main() {
    int n, i, j;
    
    // Read input
    scanf("%d", &n);

    // Loop for each row
    for (i = 0; i < n; i++) {
        
        // Print leading spaces
        for (j = 0; j < i; j++) {
            printf(" ");
        }

        // Print stars
        for (j = 0; j < n - i; j++) {
            printf("* ");
        }

        // Move to next line
        printf("\n");
    }

    return 0;
}
