#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    for (int i = n; i >= 1; i--) {
        for (int j = 1; j <= n - i + 1; j++) {
            printf("%d", i);

            // space after every number except the last
            if (!(i == 1 && j == n))
                printf(" ");
        }
    }

    return 0;
}
