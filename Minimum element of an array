#include <stdio.h>

int main() {

    int n, i;
    int arr[1000];

    scanf("%d", &n);   // read size

    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);   // read elements
    }

    int min = arr[0];   // assume first element is minimum

    for(i = 1; i < n; i++) {
        if(arr[i] < min) {
            min = arr[i];   // update minimum
        }
    }

    printf("%d", min);   // print minimum

    return 0;
}
