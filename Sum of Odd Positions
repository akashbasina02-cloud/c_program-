#include <stdio.h>

int main() {
    int n, i;
    int arr[100];
    int sum = 0;

    scanf("%d", &n);   // Read size of array

    // Read array elements
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Calculate sum of odd indexed elements
    for(i = 0; i < n; i++) {
        if(i % 2 != 0) {   // Check if index is odd
            sum += arr[i];
        }
    }

    printf("%d", sum);   // Print result

    return 0;
}
