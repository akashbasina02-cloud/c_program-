#include <stdio.h>

int main() {
    int n, i;
    scanf("%d", &n);

    int arr[n];

    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int start = 0;
    int end = n - 1;

    while(start < end) {
        printf("%d %d ", arr[start], arr[end]);
        start++;
        end--;
    }

    // If one element left (odd length)
    if(n % 2 != 0) {
        printf("%d 0", arr[start]);
    }

    return 0;
}
