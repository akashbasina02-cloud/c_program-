#include <stdio.h>

int main() {
    int n, i;
    scanf("%d", &n);

    int arr[n];
    int sum = 0;

    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }

    // Integer division works like //
    int average = sum / n;

    int count = 0;

    for(i = 0; i < n; i++) {
        if(arr[i] <= average) {
            count++;
        }
    }

    printf("%d", count);

    return 0;
}
