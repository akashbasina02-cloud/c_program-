#include <stdio.h>

int main() {

    float salary, hra, da;
    float pf, gross;

    // Read inputs
    scanf("%f", &salary);
    scanf("%f", &hra);
    scanf("%f", &da);

    // Calculate PF (12% of salary)
    pf = 0.12 * salary;

    // Calculate Gross Salary
    gross = salary + hra + da + pf;

    // Print results with 2 decimal places
    printf("%.2f\n", pf);
    printf("%.2f", gross);

    return 0;
}
