#include <stdio.h>

int main() {
    
    long long arr[8];   // Array of 8 elements
    
    // Input 8 values
    for(int i = 0; i < 8; i++) {
        scanf("%lld", &arr[i]);
    }

    long long balls = arr[2];     // Balls delivered
    long long runs = arr[3];      // Runs given
    long long wickets = arr[4];   // Wickets taken

    // Calculate Bowling Average
    double average = (double)runs / wickets;

    // Calculate Strike Rate
    double strike_rate = (double)balls / wickets;

    // Convert balls into overs
    double overs = (double)balls / 6;

    // Calculate Economy
    double economy = (double)runs / overs;

    // Print results rounded to 2 decimal places
    printf("%.2lf\n", average);
    printf("%.2lf\n", strike_rate);
    printf("%.2lf\n", economy);

    return 0;
}
