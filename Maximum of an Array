#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];

    // Read array elements
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Assume first element is maximum
    int max = arr[0];

    // Find maximum
    for(int i = 1; i < n; i++) {
        if(arr[i] > max) {
            max = arr[i];
        }
    }

    // Print maximum element
    printf("%d", max);

    return 0;
}
