#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);   // Read size of array

    int arr[n];
    int sum = 0;

    // Read array elements
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];   // Add to sum
    }

    int average = sum / n;   // Floor division (integer division in C)

    int count = 0;

    // Count elements >= average
    for(int i = 0; i < n; i++) {
        if(arr[i] >= average) {
            count++;
        }
    }

    printf("%d", count);   // Print count

    return 0;
}
